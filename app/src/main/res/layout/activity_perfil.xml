<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.perfil">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:text="6 años\nExperiencia"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="575dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:text="Ingeniero De Desarrollo De Software\nProfesión"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="498dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:text="Politécnico Grancolombiano\nInstitución"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="420dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:text="aliconnors@example.com\nPersonal"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="410dp"
        android:layout_height="wrap_content"
        android:background="#162B4E"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="-2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf=""
        />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Mi Perfil"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

    <SearchView
        android:layout_width="88dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.996"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="5dp"
        app:layout_constraintBottom_toBottomOf=""
        />

    <androidx.constraintlayout.utils.widget.ImageFilterButton
        android:id="@+id/imageFilterButton2"
        android:layout_width="165dp"
        android:layout_height="163dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.995"
        app:layout_constraintStart_toStartOf="parent"
        app:roundPercent="50"
        app:srcCompat="@drawable/ic_profile"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="62dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="   Bogotá DC, Colombia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="121dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="123dp"
        android:layout_height="22dp"
        android:text="Johnn Doe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="84dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="45dp"
        android:layout_height="54dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_menu_sort_by_size"
        tools:layout_editor_absoluteY="1dp"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="93dp"
        android:layout_height="66dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/telefono"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="244dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="95dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/profesion"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="416dp"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="96dp"
        android:layout_height="66dp"
        app:srcCompat="@drawable/CORREO"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="90dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/bookmark"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="575dp"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="91dp"
        android:layout_height="69dp"
        app:srcCompat="@drawable/userp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="498dp"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:text="(650) 555-1234\n Mobile"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="248dp"
        tools:ignore="MissingConstraints" />
        />


</androidx.constraintlayout.widget.ConstraintLayout>